{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "No-console": 2,
        "quotes": [ 2, "double" ],
        "no-unused-vars": 0,
        "comma-dangle": 2,
        "prettier/prettier": "error"
    }
}
